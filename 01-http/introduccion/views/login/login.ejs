<form action="/login" method="post">


    <label  for="usuarioId">Username</label>
    <input  id="usuarioId"
            type="text"
            name="usuario">
    <br>
    <label  for="passwordId">Password</label>
    <input  id="passwordId"
            type="password"
            name="password">
    <br>

    <button type="submit">Ingresar</button>

</form>